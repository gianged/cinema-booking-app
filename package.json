    "format:check": "prettier --check \"src/**/*.{ts,tsx,scss,css,json}\"",
    "validate": "npm run type-check && npm run lint && npm run format:check"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
